<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <!--<RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>-->
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>George Drak</Authors>
		<Company>Sitko.Ru</Company>
		<PackageDescription>Sitko.Core</PackageDescription>
		<RepositoryUrl>https://github.com/sitkoru/Sitko.Core</RepositoryUrl>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Grpc.Core.Api" Version="2.31.0" />
    <PackageReference Include="Grpc.Tools" Version="2.31.0" PrivateAssets="None" />
    <PackageReference Include="Google.Protobuf" Version="3.12.4" />
    <Protobuf Include="Proto\**\*.proto" GrpcServices="None" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\Sitko.Core.App\Sitko.Core.App.csproj" />
  </ItemGroup>

</Project>
